<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite"> <!-- by adding "parallel" here we are asking the tests run parallely. if 
		we remove that portion, the test will be run sequentially -->
	<!-- parallel="tests" -->
	<listeners>
		<listener class-name="testCases.Listeners" />
	</listeners>

	<test name="Positive test validating login">
		<classes>
			<class name="cucumberOptions.TestRunner" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
